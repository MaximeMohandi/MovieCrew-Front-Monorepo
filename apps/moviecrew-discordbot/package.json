{
  "name": "moviecrew-discordbot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "SET NODE_ENV=development && ts-node --experimental-specifier-resolution=node --esm ./src/index.ts",
    "build": "tsc",
    "test": "jest --coverage --verbose --colors",
    "test-watch": "jest --watch",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@discordx/importer": "^1.2.3",
    "@discordx/pagination": "^3.4.1",
    "@discordx/utilities": "^6.0.0",
    "discord.js": "^14.13.0",
    "discordx": "^11.7.6",
    "dotenv": "^16.3.1",
    "moviecrew-api": "*"
  },
  "devDependencies": {
    "@types/node": "*",
    "eslint-config-custom": "*",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig": "*",
    "typescript": "^5.2.2"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  }
}
